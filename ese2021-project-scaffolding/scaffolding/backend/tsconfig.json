{
"compilerOptions": {
  "moduleResolution": "node",
  "module": "commonjs",
  "target": "es6",
  "esModuleInterop": true,
  "noImplicitAny": false,
  "removeComments": true,
  "preserveConstEnums": true,
  "outDir": "./build",
  "sourceMap": true
},
"include": ["./src/**/*"],
"exclude": [
  "./tests/",
  "./node_modules/",
  "./dist/"
],
}
